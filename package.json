{
  "name": "@desertlionio/sdk",
  "version": "1.1.17",
  "description": "Desert Lion SDK",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build:local": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:prod": "NODE_ENV=prod rollup -c",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "coverage": "codecov",
    "lint": "eslint ./src/ --fix",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test:unit": "jest",
    "typecheck": "tsc --noEmit",
    "bundlesize": "bundlesize",
    "prepublishOnly": "npm run build:prod"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/desertlionio/sdk"
  },
  "author": {
    "name": "Desert Lion Team",
    "email": "team@desertlion.io",
    "url": "https://github.com/desertlionio"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "session-replay",
    "bugs",
    "time-travel-debugger",
    "debugger",
    "debug"
  ],
  "dependencies": {
    "css-selector-generator": "^3.6.1"
  },
  "devDependencies": {
    "error-stack-parser": "^2.1.4",
    "rrweb": "1.1.3",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jest": "26.0.24",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/node": "12.20.11",
    "@types/websocket": "^1.0.5",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-jest": "^28.1.1",
    "bundlesize": "0.18.1",
    "codecov": "3.8.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^16.0.1",
    "esbuild": "0.14.45",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-puppeteer": "^6.1.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "rollup": "^2.78.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dotenv": "0.3.0",
    "rollup-plugin-dts": "4.2.2",
    "rollup-plugin-esbuild": "4.9.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.7.4",
    "whatwg-fetch": "^3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "bundlesize": [
    {
      "path": "./lib/**/*.js",
      "compression": "none"
    }
  ],
  "typings": "lib/index.d.ts"
}
